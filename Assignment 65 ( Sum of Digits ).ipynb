   "source": [
    "def sum_of_digit(n):\n",
    "    s=0\n",
    "    while n>0:\n",
    "        r = n%10\n",
    "        s = s + r\n",
    "        n = n//10\n",
    "    return s\n",
    "\n",
    "n = int(input('Enter number : '))\n",
    "sum_of_digit(n)"
   ]
  }
